{
  "name": "aurelia2-bootstrap",
  "version": "2.0.0",
  "description": "A port of the Aurelia Bootstrap plugin to v2",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": "https://github.com/Vheissu/aurelia2-plugins",
  "author": "Dwayne Charrington <dwaynecharrington@gmail.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build:ts": "tsc",
    "build:other": "cpx \"./src/**/*.{html,css}\" ./dist",
    "build": "npm run build:ts && npm run build:other",
    "watch:ts": "tsc --watch --preserveWatchOutput",
    "watch:other": "cpx \"./src/**/*.{html,css}\" ./dist --watch",
    "test:debug": "cross-env DEBUG=true npm run test",
    "test:watch": "cross-env DEV=true npm run test",
    "test": "jest --config ./jest.config.cjs --passWithNoTests"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "velocity-animate": "^2.0.6"
  },
  "devDependencies": {
    "aurelia": "^2.0.0-beta.27",
    "bootstrap": "^5.3.8"
  },
  "peerDependencies": {
    "aurelia": ">=2.0.0-beta.27",
    "bootstrap": "^5.3.8"
  }
}
